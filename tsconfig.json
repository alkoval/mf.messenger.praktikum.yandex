{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es6",
    "strict": true,
    "removeComments": true,
    "outDir": "static/js",
    "sourceMap": true,
    //"outFile": "index.js",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "alwaysStrict": true,
    "strictNullChecks": true
  }
}
